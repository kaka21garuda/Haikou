#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/bukacakrawala/Desktop/haikou/node_modules/node-gyp-build/node_modules:/Users/bukacakrawala/Desktop/haikou/node_modules:/Users/bukacakrawala/Desktop/node_modules:/Users/bukacakrawala/node_modules:/Users/node_modules:/node_modules:/Users/bukacakrawala/Desktop/haikou/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/bukacakrawala/Desktop/haikou/node_modules/node-gyp-build/node_modules:/Users/bukacakrawala/Desktop/haikou/node_modules:/Users/bukacakrawala/Desktop/node_modules:/Users/bukacakrawala/node_modules:/Users/node_modules:/node_modules:/Users/bukacakrawala/Desktop/haikou/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../node-gyp-build/bin.js" "$@"
else
  exec node  "$basedir/../node-gyp-build/bin.js" "$@"
fi
